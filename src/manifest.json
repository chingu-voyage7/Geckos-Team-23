{
  "manifest_version": 2,
  "name": "Words",
  "description": "Tooltip dictionary",
  "version": "0.1",
  "author": "geckos-team-23",

  "browser_action": {
    "default_title": "Words",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "all_frames": true,
      "css": ["styles/styles.css"],
      "js": ["scripts/tippy.js", "scripts/content.js"]
    }
  ],
  "permissions": [
    "tabs",
    "activeTab"
  ]
}
